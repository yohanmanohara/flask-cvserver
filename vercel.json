{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "api/index.py",
      "methods": ["POST"]
    },
    {
      "src": "/",
      "dest": "api/index.py"
    }
  ]
}